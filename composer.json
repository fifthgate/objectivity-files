{
    "name": "fifthgate/objectivity-files",
    "description": "A Domain-Driven Files system for objectivity",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Baynham",
            "email": "sam@fifthgate.net"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "fifthgate/objectivity-users": "~2.0.1"
    },
    "require-dev": {
        "orchestra/testbench": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Fifthgate\\Objectivity\\Files\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fifthgate\\Objectivity\\Files\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/fifthgate/objectivity-core"
        },
        {
            "type": "vcs",
            "url": "https://github.com/fifthgate/objectivity-repos"
        },
        {
            "type": "vcs",
            "url": "https://github.com/fifthgate/objectivity-users"
        }
    ],

     "extra": {
        "laravel": {
            "providers": [
                "Fifthgate\\Objectivity\\Files\\Service\\FileServiceProvider"
            ]
        }
    }
}
